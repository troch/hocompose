{
  "name": "hocompose",
  "version": "1.0.0",
  "description": "Using Higher-Order Components in React is great for composition: they allow to decouple rendering logic from component lifecycle.",
  "main": "dist/commonjs/index.js",
  "jsnext:main": "dist/es/index.js",
  "devDependencies": {
    "babel-core": "~6.6.5",
    "babel-eslint": "~5.0.0",
    "babel-plugin-transform-object-rest-spread": "~6.6.5",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-es2015-rollup": "~1.1.1",
    "babel-preset-react": "^6.5.0",
    "chai": "~3.5.0",
    "enzyme": "~2.0.0",
    "eslint": "~2.2.0",
    "jsdom": "^8.1.0",
    "mocha": "~2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "rimraf": "~2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "react": "~0.14.0"
  },
  "peerDependencies": {
    "react": "~0.14.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require test_helper.js --recursive 'test/*.js'",
    "build": "rimraf dist && npm run build:es && npm run build:cjs",
    "build:es": "BABEL_ENV=es babel modules --out-dir dist/es",
    "build:cjs": "babel modules --out-dir dist/commonjs",
    "lint": "eslint modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troch/hocompose.git"
  },
  "author": "Thomas Roch <thomas.c.roch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/troch/hocompose/issues"
  },
  "homepage": "https://github.com/troch/hocompose#readme"
}
