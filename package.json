{
  "name": "hocompose",
  "version": "0.1.0",
  "description": "Using Higher-Order Components in React is great for composition: they allow to decouple rendering logic from component lifecycle.",
  "main": "dist/commonjs/index.js",
  "devDependencies": {
    "babel-core": "~6.6.5",
    "babel-eslint": "~5.0.0",
    "babel-plugin-transform-object-rest-spread": "~6.6.5",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-es2015-rollup": "~1.1.1",
    "chai": "~3.5.0",
    "enzyme": "~2.0.0",
    "eslint": "~2.2.0",
    "mocha": "~2.4.5",
    "rimraf": "~2.5.2"
  },
  "dependencies": {
    "react": "~0.14.0"
  },
  "peerDependencies": {
    "react": "~0.14.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive 'test/main.js'",
    "build": "rimraf dist && npm run build:es && npm run build:cjs",
    "build:es": "BABEL_NODE=es babel modules -o dist/es",
    "build:cjs": "babel modules -o dist/cjs",
    "lint": "eslint modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troch/hocompose.git"
  },
  "author": "Thomas Roch <thomas.c.roch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/troch/hocompose/issues"
  },
  "homepage": "https://github.com/troch/hocompose#readme"
}
